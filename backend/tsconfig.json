{
  "compilerOptions": {
    "lib": [
      "es2020"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es5",
    "rootDir": "src",
    "outDir": "lib",
    "strict": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
  ]
}
